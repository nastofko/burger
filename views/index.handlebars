<div class="container bg">

  <div class="row">
    <div class="center col-sm-12">
      <img class="pic1" src="data:image/png;base64,">
      <h2>EAT-DA-BURGER!</h2>
      
    </div>
  </div>

</div>

<div class="row">
	    <div class="col-sm-3">

			<br>
			<h2>Burgers Available To Eat!</h2>

			<ul>
			  {{#each burgers}}
			    {{#unless this.devoured}}
			      <li>
			      	{{this.id}}
			        {{this.burger_name}}
			          <button class="devouredBtn btn-primary" data-id="{{this.id}}">Devour It!</button>

			      </li>
			    {{/unless}}
			  {{/each}}
			</ul>

	    </div>

	    <div class="col-sm-3"> <img src="https://cdn0.iconfinder.com/data/icons/negative-character-traits-alphabet-s-part-1/238/negative-s009-512.png" alt="">
      </div>
<div class="col-sm-3">

			<h2>Devoured Burgers!</h2>
			
			  {{#each burgers}}
			    {{#if this.devoured}}
			      <li>
			      	{{this.id}}
			        {{this.burger_name}}

			      </li>
			    {{/if}}
			  {{/each}}
			
	    </div>
</div>


  	
	<!-- end .row -->

	<div class="row">
	    <div class="center col-sm-12">

			<h2>Add A Burger!</h2>

			<form class="create-update-form" action="/" method="POST">

			  <div class="form-group">
			    <p>Enter in your favorite burger and click Submit to Order.</p>
			    <input type="text" id="bu" name="burger_name">
			  </div>
			  
			  <button class="btn-primary" type="submit">Submit Order</button>
			</form>

	    </div>
	
  	</div>
	<!-- end .row -->

</div>